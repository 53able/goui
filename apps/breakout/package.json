{
  "name": "@goui/breakout",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch server/dev.ts",
    "build": "tsc -b && vite build && pnpm run build:ssr",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
